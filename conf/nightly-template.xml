<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.9//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.9.dtd">
<benchmark tool="goblint">

  <resultfiles>**/witness.{graphml,yml}</resultfiles>

  <option name="--conf">conf.json</option>

  <rundefinition name="SV-COMP24_no-data-race">
    <tasks name="NoDataRace-Main">
      <includesfile>SVBENCHMARKPREFIX/c/NoDataRace-Main.set</includesfile>
      <propertyfile>SVBENCHMARKPREFIX/c/properties/no-data-race.prp</propertyfile>
    </tasks>
  </rundefinition>

  <rundefinition name="SV-COMP24_valid-memcleanup">
    <tasks name="MemSafety-MemCleanup">
      <includesfile>SVBENCHMARKPREFIX/c/MemSafety-MemCleanup.set</includesfile>
      <propertyfile>SVBENCHMARKPREFIX/c/properties/valid-memcleanup.prp</propertyfile>
    </tasks>
  </rundefinition>

  <rundefinition name="SV-COMP24_termination">
    <tasks name="Termination-BitVectors">
      <includesfile>SVBENCHMARKPREFIX/c/Termination-BitVectors.set</includesfile>
      <propertyfile>SVBENCHMARKPREFIX/c/properties/termination.prp</propertyfile>
    </tasks>
    <tasks name="Termination-MainControlFlow">
      <includesfile>SVBENCHMARKPREFIX/c/Termination-MainControlFlow.set</includesfile>
      <propertyfile>SVBENCHMARKPREFIX/c/properties/termination.prp</propertyfile>
    </tasks>
    <tasks name="Termination-MainHeap">
      <includesfile>SVBENCHMARKPREFIX/c/Termination-MainHeap.set</includesfile>
      <propertyfile>SVBENCHMARKPREFIX/c/properties/termination.prp</propertyfile>
    </tasks>
    <tasks name="Termination-Other">
      <includesfile>SVBENCHMARKPREFIX/c/Termination-Other.set</includesfile>
      <propertyfile>SVBENCHMARKPREFIX/c/properties/termination.prp</propertyfile>
    </tasks>
  </rundefinition>

</benchmark>
